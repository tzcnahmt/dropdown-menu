Selector(\".trigger\");\n\n// ANIMATION ON TRIGGER (OPEN & CLOSE)\nlet isActive = false;\n\nconst tl = gsap.timeline({ paused: true });\ntl.from(header, {\n  duration: 0.5,\n  y: \"-100%\",\n  ease: Power4.easeInOut\n})\n  .from(links, {\n    // delay: 0.5,\n    y: 105,\n    stagger: 0.05\n  })\n  .from(\n    rightBackground,\n    {\n      opacity: 0\n    },\n    0.5\n  );\n\ntrigger.addEventListener(\"click\", () => {\n  if (!isActive) {\n    tl.play();\n    trigger.style.rotate = \"45deg\";\n    trigger.style = \"border-color: white\";\n    isActive = true;\n  } else {\n    tl.reverse();\n    trigger.style.rotate = \"0deg\";\n    trigger.style = \"border-color: black\";\n    isActive = false;\n  }\n});\n\n//  LINKS ANIMATION and COLOR SWITCH\nlinks.forEach((link, index) => {\n  link.addEventListener(\"mouseover\", () => {\n    header.style.backgroundColor = palette[index];\n    gsap.to(background, {\n      duration: 0.35,\n      background: palette[index]\n    });\n\n    gsap.to(contentImg[index], {\n      duration: 0.35,\n      opacity: 1\n    });\n\n    contentImg.forEach((img, ind) => {\n      if (ind !== index) {\n        gsap.to(img, {\n          opacity: 0\n        });\n      }\n    });\n  });\n});\n"},"lineCount":null}},"error":null,"hash":"659e99eac00eb973470d2a62cf20a7d0","cacheData":{"env":{}}}